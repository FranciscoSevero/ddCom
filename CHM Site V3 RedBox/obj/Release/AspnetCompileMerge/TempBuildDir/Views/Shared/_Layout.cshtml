            Layout.init(); // init layout
            Portfolio.init();            
            FormWizardPlataforma.init();
            FormWizardRecorder.init();
            ComponentsDropdowns.init();

            var timeout = '@System.Configuration.ConfigurationManager.AppSettings["Timeout"]';

            if (!isNaN(timeout)) {
                timeout = (timeout * 60);
            }
            else {
                timeout = 600;//10 minutos
            }                        

            UIIdleTimeout.init(timeout); // init session idle
        });
